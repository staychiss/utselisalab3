<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            app:layout_constraintTop_toTopOf="parent"
            android:paddingBottom="100dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="9:41"
                android:textSize="20dp"
                android:fontFamily="@font/montserrat"
                app:layout_constraintStart_toStartOf="@id/user"
                app:layout_constraintTop_toTopOf="@id/battery" />
            <ImageView
                android:id="@+id/battery"
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/battery"
                app:layout_constraintEnd_toEndOf="@id/notifikasi"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/wifi"
                android:layout_width="25dp"
                android:layout_height="25dp"
                app:layout_constraintEnd_toStartOf="@id/battery"
                app:layout_constraintTop_toTopOf="@id/battery"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/wifi"/>
            <ImageView
                android:id="@+id/cellularconnection"
                android:layout_width="25dp"
                android:layout_height="25dp"
                app:layout_constraintEnd_toStartOf="@id/wifi"
                app:layout_constraintTop_toTopOf="@id/battery"
                android:src="@drawable/cellularconnection"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"/>


            <ImageView
                android:id="@+id/user"
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="75dp"
                android:src="@drawable/ellipse" />

            <ImageView
                android:id="@+id/notifikasi"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginRight="30dp"
                android:src="@drawable/notification_icon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/user"/>

            <TextView
                android:id="@+id/hello"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="21dp"
                android:layout_marginTop="10dp"
                android:gravity="top"
                android:textSize="20dp"
                android:text="Hello, Good Morning"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/user"/>

            <TextView
                android:id="@+id/sophia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="21dp"
                android:layout_marginTop="5dp"
                android:textSize="30dp"
                android:textStyle="bold"
                android:gravity="top"
                android:text="Sophia !"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/hello"/>

            <SearchView
                android:id="@+id/searchview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/sophia"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Search"
                android:fontFamily="@font/montserrat"
                android:textSize="17dp"
                app:layout_constraintStart_toStartOf="@+id/searchview"
                app:layout_constraintTop_toBottomOf="@+id/sophia"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="32dp"
                />

            <View
                android:id="@+id/kotakorange"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:background="@drawable/kotakorange"
                app:layout_constraintTop_toBottomOf="@+id/searchview"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="65dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintEnd_toEndOf="parent"/>

            <ImageView
                android:id="@+id/girl_stretching_back"
                android:layout_width="250dp"
                android:layout_height="300dp"
                app:layout_constraintTop_toBottomOf="@+id/searchview"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="-30dp"
                android:src="@drawable/girl_stretching_back_removebg_preview_1" />

            <TextView
                android:id="@+id/fywdbst"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-60dp"
                app:layout_constraintTop_toTopOf="@id/kotakorange"
                app:layout_constraintStart_toEndOf="@id/girl_stretching_back"
                android:layout_marginTop="35dp"
                android:text="FIT YOUNG WOMAN DOING BATTLE STRETCH TRAINING"
                android:textColor="@color/white"
                android:textSize="22dp"

                android:fontFamily="@font/bebas_neue"
                tools:ignore="MissingConstraints" />
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/buttonkuning"
                android:layout_marginTop="5dp"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:backgroundTint="@color/yellow"
                android:text="Start Exercise"
                app:layout_constraintStart_toEndOf="@+id/girl_stretching_back"
                app:layout_constraintTop_toBottomOf="@+id/fywdbst"
                android:layout_marginLeft="-40dp">
            </androidx.appcompat.widget.AppCompatButton>

            <ImageView
                android:id="@+id/barbel"
                android:layout_width="100dp"
                android:layout_height="80dp"
                app:layout_constraintTop_toBottomOf="@+id/buttonkuning"
                app:layout_constraintStart_toEndOf="@+id/girl_stretching_back"
                android:layout_marginLeft="-40dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/barbel" />

            <TextView
                android:id="@+id/syg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/kotakorange"
                android:layout_marginTop="30dp"
                android:textSize="25dp"
                android:fontFamily="@font/bebas_neue"
                android:text="SELECT YOUR GOAL"/>



            <HorizontalScrollView
                android:id="@+id/syggeser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none"
                app:layout_constraintTop_toBottomOf="@+id/syg"
                app:layout_constraintStart_toStartOf="parent">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Loose Weight"
                        android:padding="8dp"
                        android:textSize="15dp"
                        android:fontFamily="@font/montserrat"
                        android:background="@drawable/bgabu"
                        android:layout_marginRight="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Gain Weight"
                        android:padding="8dp"
                        android:textSize="15dp"
                        android:fontFamily="@font/montserrat"
                        android:background="@drawable/bghitam"
                        android:textColor="@color/white"
                        android:layout_marginRight="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Body Building"
                        android:padding="8dp"
                        android:textSize="15dp"
                        android:fontFamily="@font/montserrat"
                        android:background="@drawable/bgabu"
                        android:layout_marginRight="10dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Healthy"
                        android:padding="8dp"
                        android:textSize="15dp"
                        android:fontFamily="@font/montserrat"
                        android:background="@drawable/bgabu" />

                </LinearLayout>
            </HorizontalScrollView>

            <TextView
                android:id="@+id/category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="category"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/syggeser"
                android:fontFamily="@font/bebas_neue"
                android:layout_marginTop="20dp"
                android:textSize="25dp"
                />

            <TextView
                android:id="@+id/seeall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="See all"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/category"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:textSize="18dp"
                android:fontFamily="@font/montserrat"
                />

            <HorizontalScrollView
                android:id="@+id/categorygeser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:scrollbars="none"
                app:layout_constraintTop_toBottomOf="@+id/category"
                app:layout_constraintStart_toStartOf="parent">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginRight="11dp">

                        <ImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:src="@drawable/yoga"
                            android:layout_marginRight="8dp"
                            />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Yoga"
                            android:textAlignment="center"
                            android:fontFamily="@font/montserrat"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:layout_marginTop="10dp"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginRight="11dp">
                        <ImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:src="@drawable/gym"
                            android:layout_marginRight="8dp"
                            />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Gym"
                            android:textAlignment="center"
                            android:fontFamily="@font/montserrat"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:layout_marginTop="10dp"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginRight="11dp">
                        <ImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:src="@drawable/cardio"
                            android:layout_marginRight="8dp"
                            />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Cardio"
                            android:textAlignment="center"
                            android:fontFamily="@font/montserrat"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:layout_marginTop="10dp"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:layout_marginRight="8dp"
                            android:src="@drawable/stretch" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Stretch"
                            android:textAlignment="center"
                            android:fontFamily="@font/montserrat"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:layout_marginTop="10dp"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:src="@drawable/fullbody"
                            android:layout_marginRight="8dp"
                            />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Fullbody"
                            android:textAlignment="center"
                            android:fontFamily="@font/montserrat"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:layout_marginTop="10dp"
                            />
                    </LinearLayout>
                </LinearLayout>
            </HorizontalScrollView>
            <View
                android:id="@+id/line1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/blue"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/categorygeser"/>
            <TextView
                android:id="@+id/popularexercise"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Popular Exercises"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line1"
                android:layout_marginTop="22dp"
                android:textSize="25dp"
                android:fontFamily="@font/bebas_neue" />


            <TextView
                android:id="@+id/seeall2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="See all"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/popularexercise"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:textSize="18dp"
                android:fontFamily="@font/montserrat" />

            <ImageView
                android:id="@+id/cewebarbel"
                android:layout_width="match_parent"
                android:layout_height="195dp"
                android:src="@drawable/cewebarbel"
                app:layout_constraintTop_toBottomOf="@+id/popularexercise"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:id="@+id/kotaklike"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintEnd_toEndOf="@+id/cewebarbel"
                app:layout_constraintTop_toTopOf="@+id/cewebarbel"
                android:src="@drawable/whitelove"
                android:layout_marginTop="15dp"
                android:layout_marginRight="20dp"/>
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="@+id/kotaklike"
                app:layout_constraintEnd_toEndOf="@+id/kotaklike"
                app:layout_constraintTop_toTopOf="@+id/kotaklike"
                app:layout_constraintBottom_toBottomOf="@+id/kotaklike"
                android:src="@drawable/love"/>
            <TextView
                android:id="@+id/fswsa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Full Shot Woman Stretching Arm"
                android:textSize="17dp"
                android:fontFamily="@font/montserrat"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/cewebarbel"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/beginner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Beginner"
                android:fontFamily="@font/montserrat"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@+id/fswsa"
                app:layout_constraintStart_toStartOf="parent"/>
            <View
                android:id="@+id/line2"
                android:layout_width="1dp"
                android:layout_height="15dp"
                app:layout_constraintStart_toEndOf="@+id/beginner"
                app:layout_constraintTop_toTopOf="@+id/beginner"
                android:background="@color/black"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"/>
            <ImageView
                android:id="@+id/clock"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/clock"
                android:layout_marginLeft="10dp"
                app:layout_constraintStart_toEndOf="@+id/line2"
                app:layout_constraintTop_toTopOf="@+id/line2"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="30 min"
                app:layout_constraintStart_toEndOf="@+id/clock"
                app:layout_constraintTop_toTopOf="@+id/beginner"
                android:layout_marginTop="2dp"
                android:layout_marginLeft="8dp"/>

            <View
                android:id="@+id/line3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/beginner"
                android:background="@color/gray"
                android:layout_marginTop="20dp"/>

            <ImageView
                android:id="@+id/cewematras"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:src="@drawable/cewematras"
                app:layout_constraintTop_toBottomOf="@+id/line3"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="10dp"/>
            <ImageView
                android:id="@+id/kotaklike2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintEnd_toEndOf="@+id/cewematras"
                app:layout_constraintTop_toTopOf="@+id/cewematras"
                android:src="@drawable/whitelove"
                android:layout_marginTop="15dp"
                android:layout_marginRight="23dp"/>

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="@+id/kotaklike2"
                app:layout_constraintEnd_toEndOf="@+id/kotaklike2"
                app:layout_constraintTop_toTopOf="@+id/kotaklike2"
                app:layout_constraintBottom_toBottomOf="@+id/kotaklike2"
                android:src="@drawable/love"/>
            <TextView
                android:id="@+id/apmmatras"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Athlete Practicing Monochrome"
                android:textSize="17dp"
                android:fontFamily="@font/montserrat"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/cewematras"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/beginner1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Beginner"
                android:fontFamily="@font/montserrat"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@+id/apmmatras"
                app:layout_constraintStart_toStartOf="parent"/>
            <View
                android:id="@+id/line4"
                android:layout_width="1dp"
                android:layout_height="15dp"
                app:layout_constraintStart_toEndOf="@+id/beginner1"
                app:layout_constraintTop_toTopOf="@+id/beginner1"
                android:background="@color/black"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"/>
            <ImageView
                android:id="@+id/clock1"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/clock"
                android:layout_marginLeft="10dp"
                app:layout_constraintStart_toEndOf="@+id/line4"
                app:layout_constraintTop_toTopOf="@+id/line4"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="50 min"
                app:layout_constraintStart_toEndOf="@+id/clock1"
                app:layout_constraintTop_toTopOf="@+id/beginner1"
                android:layout_marginTop="2dp"
                android:layout_marginLeft="8dp"/>
            <View
                android:id="@+id/line5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/beginner1"
                android:background="@color/gray"
                android:layout_marginTop="20dp"/>

            <TextView
                android:id="@+id/meal_plans"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Meal Plans"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/line5"
                android:layout_marginTop="22dp"
                android:textSize="25dp"
                android:fontFamily="@font/bebas_neue" />



            <TextView
                android:id="@+id/seeall3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="See all"
                app:layout_constraintBottom_toBottomOf="@+id/meal_plans"
                app:layout_constraintEnd_toEndOf="parent"
                android:textStyle="bold"
                android:textSize="18dp"
                android:fontFamily="@font/montserrat" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="300dp"
                app:layout_constraintTop_toBottomOf="@+id/meal_plans">
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycler_container"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </androidx.recyclerview.widget.RecyclerView>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/additionalexercise"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Additional Exercise"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/meal_plans"
                android:layout_marginTop="300dp"
                android:textSize="25dp"
                android:fontFamily="@font/bebas_neue" />


            <TextView
                android:id="@+id/seeall4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="See all"
                app:layout_constraintTop_toTopOf="@+id/additionalexercise"
                app:layout_constraintEnd_toEndOf="parent"
                android:textStyle="bold"
                android:textSize="18dp"
                android:fontFamily="@font/montserrat" />

            <ImageView
                android:id="@+id/jumprope"
                android:layout_width="120dp"
                android:layout_height="120dp"
                app:layout_constraintTop_toBottomOf="@+id/additionalexercise"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="20dp"
                android:src="@drawable/jumprope"
                android:background="@drawable/kotakorange1"/>

            <ImageView
                android:id="@+id/ropeditmpt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/ropeditmpt"
                android:background="@drawable/kotakbiru2"
                app:layout_constraintTop_toBottomOf="@+id/jumprope"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="10dp"/>

            <ImageView
                android:id="@+id/ceweangkatbarbel"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/ceweangkatbarbel"
                android:background="@drawable/kotakpinkoren"
                app:layout_constraintTop_toBottomOf="@+id/ropeditmpt"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/ewjr"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:text="Exercise with Jumping Rope"
                android:fontFamily="@font/montserrat"
                app:layout_constraintStart_toEndOf="@+id/jumprope"
                app:layout_constraintTop_toTopOf="@+id/jumprope"
                android:maxLines="2"
                android:layout_marginLeft="20dp"
                android:textStyle="bold"
                android:textSize="15dp"/>

            <ImageView
                android:id="@+id/api1"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/calories"
                app:layout_constraintStart_toEndOf="@+id/jumprope"
                app:layout_constraintTop_toBottomOf="@+id/ewjr"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"/>



